import java.util.Scanner;

public class sicaklik {
    public static void main(String[] args) {
        int heat;

        Scanner input = new Scanner(System.in);

        System.out.print("Hava kaç derece: ");
        heat = input.nextInt();

        if((heat<5)) {
            System.out.print("kayak yapmaya gidebilirsiniz");
        }else if(heat >= 5 && heat<= 25){
            if (heat<=15){
                System.out.print("Sinemaya gidebilirsin " );
            }
            if(heat >=10){
                System.out.print("Pikniğe gidebilirsin");
            }else{
                System.out.print("Yüzmeye gidebilirsin ");

            }
        }{

        }





        }
        }
