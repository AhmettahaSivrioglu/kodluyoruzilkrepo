import java.util.Scanner;

public class siralama {
    public static void main(String[] args) {
        int a, b, c;

        Scanner input = new Scanner(System.in);

        // Sayıları kucukten buyuge sıralayan program.

        System.out.print("a sayısını giriniz :");
        a = input.nextInt();
        System.out.print("b sayısını giriniz :");
        b = input.nextInt();
        System.out.print("c sayısını giriniz :");
        c = input.nextInt();



    if((a>b)&&(a>c)){
        if(b>c){
            System.out.print("a en büyük sayıdır.\nb ortanca sayıdır.\nc en küçük sayıdır.");

        }else {
            System.out.print("a en büyük sayıdır.\nc ortanca sayıdır.\nb en küçük sayıdır.");
        }
    }else if((b>a)&&(b>c)){
        if(a>c){
            System.out.print("b en büyük sayıdır.\na ortanca sayıdır.\nc en küçük sayıdır.");

        }else {
            System.out.print("b en büyük sayıdır.\nc ortanca sayıdır\na en küçük sayıdır.");

    }}else if((c>a)&&(c>b)){
        if(a>b){
            System.out.print("c en büyük sayıdır.\na ortanca sayıdır.\nb en küçük sayıdır.");

        }else {
            System.out.print("c en büyük sayıdır.\nb ortanca sayıdır.\na en küçük sayıdır.");

        }

        }












    }

                }
