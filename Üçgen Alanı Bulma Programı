package Giris;

import java.util.Scanner;

public class Proje03 {
    public static void main(String[] args) {

        int x,y ;
        double z;


        Scanner input = new Scanner (System.in);

        System.out.print("birinci kenar degeri: ");
        x= input.nextInt();
        System.out.print("ikinci kenar degeri : ");
        y = input.nextInt();

        z=Math.sqrt(x*x)+(y*y);

        System.out.println("Ucgen AlanÄ± = " + z);
       






    }
}
